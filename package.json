{
  "name": "brewskey.js-api",
  "version": "0.2.0-SNAPSHOT",
  "main": "./build/index.js",
  "repository": "https://github.com/Brewskey/brewskey.js-api.git",
  "author": "Brewskey",
  "files": ["build/"],
  "scripts": {
    "build": "babel src -d build --copy-files",
    "flow": "flow",
    "lint": "eslint src/ --fix --max-warnings 0",
    "precommit": "lint-staged",
    "start": "babel ./src --out-dir ./build --watch --copy-files",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-sorting": "^0.3.0",
    "flow-bin": "^0.59.0",
    "form-data": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "minimist": "^1.2.0",
    "node-fetch": "^1.7.3",
    "prettier": "^1.8.2"
  },
  "dependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "immutable": "4.0.0-rc.9",
    "nullthrows": "^1.0.0",
    "odata": "git+https://github.com/Brewskey/o.js.git"
  },
  "ava": {
    "verbose": true,
    "babel": "inherit",
    "files": ["test/*.test.js"],
    "require": ["babel-register"]
  },
  "lint-staged": {
    "src/**/*.{js,json}": ["prettier --write", "lint", "git add"]
  }
}
